# Lista de datos
datos_componentes = [
    ("Gelatin", "3 w/v %", "0.3 g in 10 mL"),
    ("Sodium Alginate", "3 w/v %", "0.3 g in 10 mL")
]

# Crear el diccionario vac√≠o
datos = {}

# Llenar el diccionario en un bucle
for i, (nombre, cantidad, detalle) in enumerate(datos_componentes, 1):
    datos[f"Componente {i}"] = {
        "nombre": nombre,
        "cantidad": cantidad,
        "detalle": detalle
    }

# Imprimir el diccionario
print(datos)
